<?xml version="1.0" encoding="UTF-8"?>
<!--
maxPersonalIncomeTax: 最大应纳所得税额度阀值
minPersonalIncomeTax: 最小应纳所得税额度阀值
taxRate: 适用税率
deductCount: 速算扣除数
-->
<taxRate>
	<rate>
		<maxPersonalIncomeTax>20000</maxPersonalIncomeTax>
		<minPersonalIncomeTax>0</minPersonalIncomeTax>
		<taxRate>0.2</taxRate>
		<deductCount>0</deductCount>
	</rate>
	<rate>
		<maxPersonalIncomeTax>50000</maxPersonalIncomeTax>
		<minPersonalIncomeTax>20000</minPersonalIncomeTax>
		<taxRate>0.3</taxRate>
		<deductCount>2000</deductCount>
	</rate>
	<rate>
		<minPersonalIncomeTax>50000</minPersonalIncomeTax>
		<taxRate>0.4</taxRate>
		<deductCount>7000</deductCount>
	</rate>
</taxRate>