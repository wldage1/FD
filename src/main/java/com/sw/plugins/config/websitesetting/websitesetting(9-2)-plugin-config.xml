<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	index: 权限英文编码
	code:权限代码
	name:权限中文描述
	level:权限层级 
	isLog:是否需要写入系统日志（true/false）
	isNav:是否有导航信息（true/false）
	isSetAuth:是否具有下级子权限
	isStatus:是否记录状态
	controller:权限控制URL
-->
<plugins>
	<config name="系统配置" icon="images/menu/system-configuration.png">
		<websitesetting isNav="true" isSetAuth="sub" isStatus="true" name="网站设置" icon="images/menu/system-configuration-website design.png">
			<language>
				<value>/language/websitesettingMessages_zh_CN.properties</value>
			</language>
			<modify isNav="true" isLog="true" name="修改" />
			<save isNav="true" isLog="true" name="保存" />
			<cancel isNav="true" isLog="true" name="取消" /> 
			<webModify isNav="true" isLog="true" name="修改" />
			<logoModify isNav="true" isLog="true" name="修改" />
		</websitesetting>
	</config>
</plugins>
