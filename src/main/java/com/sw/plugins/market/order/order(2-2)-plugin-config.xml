<!--				<pushShare isNav="true" isLog="true" name="额度配给" icon="images/icon/create.png"/> -->
				<bankStreamConfirm isNav="false" isLog="true" name="银行流水确认" icon="images/icon/create.png">
					<addto isNav="true"  name="金额确认" icon="images/icon/detail.png" />
					<recheck isNav="true"  name="复核" icon="images/icon/detail.png" />
					<modifyTwo isNav="true"  name="修改" icon="images/icon/detail.png" />
					<cancel isNav="true"  name="取消" icon="images/icon/detail.png" />
